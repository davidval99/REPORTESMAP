{
  "expo": {
    "name": "Desfogues Costa Rica ",
    "slug": "reportescr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Esta aplicación permite a los usuarios realizar reportes acerca de desfogues y botaderos a los ríos. Estos reportes serán revisados por las autoridades municipales y de salud correspondientes con el fin de limpiarlos."
  }
}
